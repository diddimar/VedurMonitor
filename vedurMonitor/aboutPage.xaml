<UserControl x:Class="VedurMonitor.AboutPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VedurMonitor"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="325" Loaded="UserControl_about_Loaded" Background="Transparent">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./ImageFolder/DictionaryMyndir.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <Image Margin="10" Source="{DynamicResource TrueAbout}" Opacity="0.75"/>
    </Grid>
</UserControl>
